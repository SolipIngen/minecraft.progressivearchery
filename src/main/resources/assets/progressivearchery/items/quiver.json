{
  "model": {
    "type": "minecraft:select",
    "cases": [
      {
        "model": {
          "type": "minecraft:condition",
          "on_false": {
            "type": "minecraft:condition",
            "on_false": {
              "type": "minecraft:model",
              "model": "progressivearchery:item/quiver"
            },
            "on_true": {
              "type": "minecraft:model",
              "model": "progressivearchery:item/quiver_filled"
            },
            "property": "progressivearchery:quiver/has_item"
          },
          "on_true": {
            "type": "minecraft:composite",
            "models": [
              {
                "type": "minecraft:model",
                "model": "progressivearchery:item/quiver"
              },
              {
                "type": "progressivearchery:quiver/selected_item"
              },
              {
                "type": "minecraft:model",
                "model": "progressivearchery:item/quiver"
              }
            ]
          },
          "property": "progressivearchery:quiver/has_selected_item"
        },
        "when": "gui"
      },
      {
        "model": {
          "type": "minecraft:condition",
          "on_false": {
            "type": "minecraft:model",
            "model": "progressivearchery:item/quiver"
          },
          "on_true": {
            "type": "minecraft:model",
            "model": "progressivearchery:item/quiver_filled"
          },
          "property": "progressivearchery:quiver/has_item"
        },
        "when": [
          "ground",
          "fixed"
        ]
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "progressivearchery:item/quiver"
    },
    "property": "minecraft:display_context"
  }
}